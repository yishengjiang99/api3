{
  "name": "grepawk",
  "version": "1.0.1",
  "description": "simple websockets for reading and writing into azure blob service",
  "main": "dist/index.js",
  "author": "Yisheng Jiang",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^26.0.7",
    "axios": "^0.19.2",
    "azure-storage": "^2.10.3",
    "bufferutil": "^4.0.1",
    "chalk": "^4.1.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-react-forked": "^0.1.0",
    "html-typings": "^1.4.4",
    "mime": "^2.4.6",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "ws": "^7.3.1",
    "xaa": "^1.6.0",
    "xclap": "^0.2.51"
  },
  "scripts": {
    "start": "ts-node https.ts",
    "tunnel": "sh -c 'ssh -L 3306:127.0.0.1:3306 AzureUser@grepawk.com > tunnel.log 2> tunnel_err.log < /dev/null'",
    "staging": "rsync -vpazh --exclude 'node_modules' . AzureUser@grepawk.com:/home/AzureUser/api3-staging",
    "test:staging": "ssh AzureUser@grepawk.com \"cd /home/AzureUser/api3-staging ; npm update ; npm test\"",
    "start:sig": "node --inspect=5858 -r ts-node/register ./src/signal.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "test": "jest",
    "test:db": "jest src/db.test.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "files": [
    "dist",
    "lib"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.20",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.1"
  }
}
